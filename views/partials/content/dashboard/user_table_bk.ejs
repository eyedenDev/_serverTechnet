
<div class="clearfix"></div>
<!-- DataTables Example -->
          <div class="card mb-3">
            <div class="card-header">TechNet Users</div>
            <div class="card-body">
              <div class="table-responsive">
                <table class="table table-striped table-hover table-bordered" width="100%" cellspacing="0">
                  <thead>
                    <tr class="text-center">
                      <th scope="col"></th>
                      <th scope="col">First Name</th>
                      <th scope="col">Last Name</th>
                      <th scope="col">Email</th>
                      <th scope="col">User type</th>
                      <th scope="col">Actions</th>
                    </tr>
                  </thead>
                  <tbody>
                    <% users.forEach((dispUser, index) => {

                      let on_off_line = dispUser.loggedIn == 1 ? `<i class="fas fa-toggle-on"></i>`: `<i class="fas fa-toggle-off"></i>`;
                     %>
                      <tr class="text-center">
                          <th scope="row" class="align-middle"><%- on_off_line %></th>
                          <td class="align-middle"><%= dispUser.first_name %></td>
                          <td class="align-middle"><%= dispUser.last_name %></td>
                          <td class="align-middle"><%= dispUser.email %></td>
                          <td class="align-middle"><%= dispUser.account_type %></td>
                          <td class="align-middle">
                            <!--  -->
                              <a href="/api/user/<%= dispUser.id %>?action=update" class="btn btn-sm btn-warning"><i class="fas fa-pen-square"></i></a>
                              <a href="/api/user/<%= dispUser.id %>?action=delete" class="btn btn-sm btn-danger"><i class="fas fa-trash-alt"></i></a>
                          </td>
                      </tr>
                    <% }) %>
                  </tbody>
                </table>
              </div>
            </div>
            <!-- <div class="card-footer small text-muted">Updated yesterday at 11:59 PM</div> -->
          </div>
